t = 0;
nOfRelaxedConstraints = 2 * dimX * dimY;
u = zeros(1, nOfRelaxedConstraints);
theta = 2;
thetaCount = 0;
nrOfIterations = 1000;
gammaT = [];

for iteration = 1:nrOfIterations
    SolveLagrangeanSubProblem(u);
    gammaT = CalculateSubGradientDirection();
    alpha = theta(k-)
    %theta Ã¤r en step length parameter, inget mr speciellt
    u = max(0,u+alpha*max(0,gammaT))
    t = t + 1;
    if mod(iteration, 10) == 0
        theta = theta * 0.95;
    end
